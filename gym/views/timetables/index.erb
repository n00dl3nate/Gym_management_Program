<h1>Timetables</h1>
<a href="/timetables/new"><li>Add New Time For Class</li></a>
  <table>
    <tr>
      <th>Name</th>
      <th>Time</th>
      <th>Spaces Left</th>
      <th>Delete</th>
    </tr>
      <% for session in @timetables%>
      <tr>
        <td><%= session['name'] %></td>
        <td><%= session['session_time'] %></td>
        <td><%= session['capacity'] %> </td>
        <td>
          <form action="/timetables/<%= session['id'] %>/delete" method="post">
            <input class="interact interact-delete" type="submit" value="Delete">
          </form>
        </td>
      <% end %>
      </tr>
  </table>
