


<h1></h1>
<h1>Members</h1>





<div class = "members">
    <a class"members" href="/members/new"><li>Add Member</li></a>
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Premium</th>
    <th>Fitness Level</th>
    <th>View</th>

  </tr>
    <% for member in @members%>
    <tr>
      <td><%= member.f_name %></td>
      <td><%= member.l_name %></td>
      <td><%= member.premium %></td>
      <td><%= member.fitness_level %></td>
      <td><a href="/members/<%= member.id %>" class="view">View</a></td>
    </tr>
  <% end %>
</table>
